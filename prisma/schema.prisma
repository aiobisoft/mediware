// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Supplier {
  id Int @id @default(autoincrement())
  name String
  city String
  addressLine1 String
  telephones String
  emails String
  addressLine2 String?
  whatsapps String?
  NTN String?
  STN String?
  licenseNumber String?
  TNNumber String?
  TRNNumber String?
}